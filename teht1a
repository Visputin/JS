const prompt=require("prompt-sync")({sigint: true});

var num1 = prompt("Anna 1. luku: ");
var num2 = prompt("Anna 2. luku: ");

if (num1 > num2) {
    console.log(num1+" on suurempi luku");
}
else if (num2 > num1) {
    console.log(num2+" on suurempi luku");
}
else {
    console.log("Luvut ovat yht√§ suuret");
}